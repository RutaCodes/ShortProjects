*HW4* RUTA BASIJOKAITE*;
PROC IMPORT DATAFILE="/folders/myfolders/HW4/Dummy.xlsx" /** Import an XLSX file.  **/
		    OUT=WORK.HW4DATA
		    DBMS=XLSX
		    REPLACE;
		    GETNAMES=YES;
		    SCANTEXT=YES;
RUN;
OPTIONS NOCENTER NODATE PAGENO=1 LS=76 PS=45 NOLABEL;
DATA ALL;
  SET HW4DATA;
  IF SEX='female' THEN Z=0;
  ELSE Z=1;
  XZ=X*Z;
RUN;
PROC MEANS N MEAN MEDIAN STD MIN MAX MAXDEC=4;
   VAR COLLEGE;
   BY Z;
   TITLE 'DESCRIPTIVE STATISTICS';
 RUN;
ODS GRAPHICS ON / ATTRPRIORITY=NONE;
PROC SGPLOT;
  STYLEATTRS DATACOLORS=(BLUE RED) DATASYMBOLS=(CIRCLEFILLED SQUAREFILLED);
  SCATTER X=X Y=COLLEGE / GROUP=Z;
  REG X=X Y=COLLEGE/ GROUP=Z;
  TITLE 'COLLEGE ADMISSION DURING YEARS 1975-1993';
RUN;
ODS GRAPHICS OFF;
*REGRESSION ANALYSIS;
PROC REG DATA=ALL OUTEST=M1;
  MODEL COLLEGE= X;
  TITLE 'REGRESSION MODEL COMBINING MALE AND FEMALE DATA';
RUN;
PROC REG DATA=ALL OUTEST=M2;
  MODEL COLLEGE= X;
  BY Z;
  TITLE 'SEPARATE REGRESSION MODELS';
RUN;
PROC REG DATA=ALL OUTEST=M3;
  MODEL COLLEGE= X Z XZ;
  INTERCEPT: TEST Z;
  SLOPE: TEST XZ;
  COINCIDENCE: TEST Z, XZ;
  TITLE 'REGRESSION MODELS WITH DUMMY VARIABLE Z';
RUN;
